pr = Course(id=1, name='Проектирование', description='Этот курс посвящен тому, чтобы ты научился планировать '
                                                             'и оценивать свои задумки, составлять требования к '
                                                             'будующим проектам, научился строить макеты и прототипы, '
                                                             'а также их тестировать!')
        db.session.add(pr)
        db.session.commit()
        prbon1 = Bonuses(description='10 подробных лекций с материалами', crs=pr)
        prbon2 = Bonuses(description='Сертификат о прохождении курса', crs=pr)
        prbon3 = Bonuses(description='Коллекцию новых работ в свое портфолио', crs=pr)
        db.session.add(prbon1)
        db.session.add(prbon2)
        db.session.add(prbon3)